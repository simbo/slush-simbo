    "csswring": "^3.0.3",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-config-sync": "^1.0.1",
    "gulp-connect": "^2.2.0",
    "gulp-csslint": "^0.1.5",
    "gulp-ext-replace": "^0.1.0",
    "gulp-if": "^1.2.5",
    "gulp-jshint": "^1.10.0",
    "gulp-postcss": "^5.0.1",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-stylus": "^2.0.1",
    "gulp-svgmin": "^1.1.1",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "jquery": "^2.1.3",
    "lodash": "^3.6.0",
